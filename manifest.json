{
  "manifest_version": 2,
  "name" : "Google Docs Quick Create",
  "version": "0.1.3",
  "description": "Create new Documents for Google Drive from your browser bar.",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icons/suite_38.png"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "128": "icons/suite_128.png",
    "48": "icons/suite_48.png",
    "16": "icons/suite_16.png"
  }
}